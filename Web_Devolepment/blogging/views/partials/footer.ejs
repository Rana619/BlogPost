<script>
  (function()
  {
    $('form > div > input').keyup(function(){
       var empty = false;
       $('form > div > input').each(function(){
           if($(this).val()==''){
             empty = true; 
           }
       });

       if(empty){
       $('#submit').attr ('disabled', 'disabled');
    }
    else{
      $('#submit').removeAttr('disabled');
    }
    });
  })()
